	<script>
	$(document).ready(function() {
		$("body").on("touchstart", function(evt) {
		
			touchStart = evt.targetTouches[0];
			
		
		});
		
		$("body").on("touchend", function(evt) {
		
			touchEnd = evt.targetTouches[0];
			
			if((touchEnd.pageX - touchStart.pageX) < -300) {
				window.location("#");
			}
			else if((touchEnd.pageX - touchStart.pageX) > 300) {
				window.location("about.html");
			}
			alert("stopped touched");
		});
	});
	</script>
