	&lt;StatusCode&gt;&lt;/StatusCode&gt;
	&lt;TransactionDate&gt;&lt;/TransactionDate&gt;
	&lt;Transactions&gt;
		&lt;Transaction&gt;
			&lt;ItemType&gt;&lt;/ItemType&gt;
			&lt;ConfirmationNumber&gt;&lt;/ConfirmationNumber&gt;
