body {

	height: 100%;
	/*border-top: 3px solid #48a942; CSI Green*/
	font-family: 'Arimo', sans-serif;
	border-top: 10px dimGray solid;
}

h2 > a {
	cursor: pointer;
}
/***** Main container to center items and give a relative space to position from */
#container {

	/* Position of self */
	margin: 0 auto; /*Centers the element */
	padding-bottom: 20px;

	/* Size */
	width: 960px;
	height: auto;
	
	/* Color */
	/*background: #ededed;
	box-shadow: 0 0 5px #5f5f5f;*/
	
	/* Positioning of child elements */
	position: relative;
	
}

	#container > header {
		margin-top: 10px;
		height: 150px;
		position: relative;
	}
	
	#container > header > ul {
		width: 695px;
		margin-left: 265px;
		margin-bottom: 0px;
		padding-left: 5px;
		display: inline-block;
		position: absolute;
		background: white;
		z-index: 999;
		box-shadow: 0 8px 6px -6px #6E6E6E;
	}
	
	#container > header > ul.top {
		top: 0px;
		padding-top: 12px;
	}
	
	#container > header > ul.bottom {
		bottom: 3px;
	}
	
	#logo {
		width: 244px;
		display: inline-block;
		position: absolute;
		background: #5EC47F;
		border-radius: 3px;
		border: #ededed solid 3px;
		text-align: center;
	}
	
	#logo img {
		height: 144px;
	}
	
	#container > header > h1 {
		margin-left: 265px;
		padding-top: 55px;
		font-size: 3.5em;
	}
	#container > header > p {
		position: absolute;
		font-size: 0.8em;
		top: 0px;
		right: 5px;
	}
	
	#contentContainer {
		width: 695px;
		margin-left: 265px;
		margin-top: 15px;
		padding-bottom: 15px;
	}
	
	#contentContainer > article > h1 > a {
		color: #08C;
		cursor: pointer;
	}
	
	#contentContainer > article > section  {
		margin-left: 10px;
	}
	
	#contentContainer > article > section > p {
		font-size: 0.9em;
	}
	
	#contentContainer > article > section > h4 > a {
		color: black;
		cursor: pointer;
	}
	
	#contentContainer > article > section > code {
	}
	
	span.red {
		color: red !important;
	}
	
	span.copy {
		color: blue;
		font-size: 0.8em;
		cursor: pointer;
		position: absolute;
		bottom: 0px;
		right: 15px;
	}
	
	span.copy:hover {
		text-decoration: underline;
	}
	
	.tab-content {
		position: relative;
	}
	
	#sideNav {
		-webkit-box-shadow: rgba(0, 0, 0, 0.0627451) 0px 1px 4px 0px;
		background-color: white;
		border-radius: 6px;
		box-shadow: rgba(0, 0, 0, 0.0627451) 0px 1px 4px 0px;
		color: #333;
		display: block;
		font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
		font-size: 14px;
		height: auto;
		line-height: 20px;
		list-style-image: none;
		list-style-position: outside;
		list-style-type: none;
		margin-bottom: 0px;
		margin-left: 0px;
		margin-right: 0px;
		margin-top: 30px;
		padding: 0px;
		
		position: absolute;
		top: 0px;
		width: 250px;
		z-index: 1;
	}
	
	#sideNav > li {
		background-color: transparent;
		position: relative;
		color: #333;
		display: list-item;
		font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
		font-size: 14px;
		height: 38px;
		line-height: 20px;
		list-style-image: none;
		list-style-position: outside;
		list-style-type: none;
		margin-bottom: 0px;
		margin-left: 0px;
		margin-right: 0px;
		margin-top: 0px;
		text-align: left;
		width: 250px;
	}
	
	#sideNav > li > a {
		border: #E5E5E5 solid 1px;
		background: white;
		color: #08C;
		cursor: pointer;
		display: block;
		font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
		font-size: 14px;
		height: 20px;
		margin-bottom: -1px;
		margin-left: 0px;
		margin-right: 0px;
		margin-top: -1px;
		padding-bottom: 8px;
		padding-left: 14px;
		padding-right: 14px;
		padding-top: 8px;
		text-align: left;
		text-decoration: none;
		text-shadow: rgba(255, 255, 255, 0.498039) 0px 1px 0px;
		width: 220px;
	}
	
	#sideNav > li > i {
		height: 13px;
		width: 13px;
		background-image: url('../img/arrow.png');
		background-position: 1px 0px;
		background-color: white;
		position: absolute;
		border-radius: 10px;
		top: 9px;
		right: 5px;
		cursor: pointer;
		border: 3px solid white;
	}
	
	#sideNav > li > i:hover {
		background-color: #B0E5FF;
		border-color: #B0E5FF;
	}
	
	#sideNav > li > i.active {
		background-position: 13px 0px;
	}
	
	#sideNav > li > ul {
		display: block;
		position: absolute;
		list-style: none;
		top: 0px;
		left: 0px;
		margin: 0px;
		z-index: -1;
		-webkit-box-shadow: rgba(0, 0, 0, 0.0627451) 0px 1px 4px 0px;
		box-shadow: rgba(0, 0, 0, 0.0627451) 0px 1px 4px 0px;
		color: #333;
		display: block;
		font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
		font-size: 14px;
		height: auto;
		line-height: 20px;
		list-style-image: none;
		list-style-position: outside;
		list-style-type: none;
		margin-bottom: 0px;
		margin-left: 0px;
		margin-right: 0px;
		padding: 0px;
		
	}
	
	#sideNav > li > ul > li {
		
		position: relative;
		color: #333;
		display: list-item;
		font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
		font-size: 14px;
		height: 38px;
		line-height: 20px;
		list-style-image: none;
		list-style-position: outside;
		list-style-type: none;
		margin-bottom: 0px;
		margin-left: 0px;
		margin-right: 0px;
		margin-top: 0px;
		text-align: left;
		width: 250px;
	}
	
	#sideNav > li > ul > li > a {
		background-color: whiteSmoke;
		border: #E5E5E5 solid 1px;
		color: #08C;
		cursor: auto;
		display: block;
		font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
		font-size: 14px;
		height: 20px;
		margin-bottom: -1px;
		margin-left: 0px;
		margin-right: 0px;
		margin-top: -1px;
		padding-bottom: 8px;
		padding-left: 14px;
		padding-right: 14px;
		padding-top: 8px;
		text-align: left;
		text-decoration: none;
		text-shadow: rgba(255, 255, 255, 0.498039) 0px 1px 0px;
		width: 220px;
	}
	
	#sideNav > li > ul > li > a:hover {
		background-color: white;
	}
	
	#sideNav a.active {
		color: white;
		text-shadow: 0 1px 0 rgba(0, 0, 0, .15);
		background: #08C;
	}
	
	#sideNav a.active:hover {
		color: white;
		text-shadow: 0 1px 0 rgba(0, 0, 0, .15);
		background: #08C;
	}
	
	
	#sideNav  li:nth-child(1) > a {
		border-top-left-radius: 6px;
		border-top-right-radius: 6px;
	}
	
	#sideNav  li:last-child > a {
		border-bottom-left-radius: 6px;
		border-bottom-right-radius: 6px;
	}
	
	#sideNav  li > a:hover {
		background-color: whiteSmoke;
	}
	
pre {
	margin-left: 20px !important;
	border: none !important;
	
	white-space: pre;
	word-wrap: normal;
}

ul.no-list-style {
	
	list-style: none;

}
table > thead > tr > td[colspan] {
	text-align: center; 
}

#contentContainer > article > section > code:nth-last-child(1) {
	margin-bottom: 10px;
}

* {
		transition: background-color 0.25s, border-color 0.25s, color 0.25s;
		-moz-transition: background-color 0.25s, border-color 0.25s, color 0.25s;
		-webkit-transition: background-color 0.25s, border-color 0.25s, color 0.25s;
		-o-transition: background-color 0.25s, border-color 0.25s, color 0.25s;
}

.btn-large [class^="icon-"], .btn-large [class*=" icon-"] {
margin-top: 4px;
}

h2 > a {
	color: #000;
	font-size: 0.5em;
}

h2 {
	font-size: 1.5em;
}

a[target=_blank]:after {

	content: "\00b1";
	color: red;
	background: #ededed;
	border: 1px #ccc solid;
	margin-top: 2px;

}